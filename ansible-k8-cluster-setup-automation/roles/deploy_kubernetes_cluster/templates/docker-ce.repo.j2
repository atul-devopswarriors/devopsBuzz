[docker-ce-stable]
name=Docker CE Stable - {{ansible_architecture}}
baseurl=https://download.docker.com/linux/{{ ansible_distribution }}/{{ansible_distribution_major_version}}/{{ansible_architecture}}/stable
enabled=1
gpgcheck=1
gpgkey=https://download.docker.com/linux/{{ ansible_distribution }}/gpg

[docker-ce-stable-debuginfo]
name=Docker CE Stable - Debuginfo {{ ansible_architecture }}
baseurl=https://download.docker.com/linux/{{ ansible_distribution }}/{{ansible_distribution_major_version}}/debug-{{ ansible_architecture }}/stable
enabled=0
gpgcheck=1
gpgkey=https://download.docker.com/linux/{{ ansible_distribution }}/gpg

